{
  "name": "iamdocuman",
  "version": "1.0.0",
  "description": "Document management system, complete with roles and privileges. Each document defines access rights; the document defines which roles can access it. Also, each document specifies the date it was published.",
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --opts server/tests/mocha.opts server/tests/server/**/*.js",
    "start": "npm run migrate && nodemon server.js --exec babel-node",
    "migrate": "sequelize db:migrate",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev:hot": "webpack-dev-server --hot --inline --progress --colors --watch --display-error-details --display-cached --content-base ./",
    "test:frontend": "NODE_ENV=test mocha -w client/test/helpers/browser.js client/test/**/*.spec.js --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-iamao/iamdocuman.git"
  },
  "keywords": [
    "document",
    "management",
    "system"
  ],
  "author": "Inumidun Amao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-iamao/iamdocuman/issues"
  },
  "homepage": "https://github.com/andela-iamao/iamdocuman#readme",
