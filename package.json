{
  "name": "documan",
  "version": "1.0.0",
  "description": "Document management system, complete with roles and privileges. Each document defines access rights; the document defines which roles can access it. Also, each document specifies the date it was published.",
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha tests/server/**/*.js --require tests/helpers --reporter spec --compilers js:babel-register --slow 5000",
    "start": "babel-node server.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-iamao/documan.git"
  },
  "keywords": [
    "document",
    "management",
    "system"
  ],
  "author": "Inumidun Amao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-iamao/documan/issues"
  },
  "homepage": "https://github.com/andela-iamao/documan#readme",
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "body-parser": "^1.17.1",
    "consign": "^0.1.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "supertest": "^3.0.0"
  }
}
